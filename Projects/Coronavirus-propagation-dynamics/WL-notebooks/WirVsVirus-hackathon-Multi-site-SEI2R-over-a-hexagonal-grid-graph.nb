      RowBox[{"KeyValueMap", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"#1", "\[Rule]", 
          RowBox[{"#2", "[", "\"\<Center\>\"", "]"}]}], "&"}], ",", 
        "aCells"}], "]"}]}], ",", 
     RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
     RowBox[{"VertexLabels", "\[Rule]", 
      RowBox[{"Placed", "[", 
       RowBox[{"Automatic", ",", "Center"}], "]"}]}], ",", 
     RowBox[{"VertexSize", "\[Rule]", " ", ".4"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"If", "[", 
